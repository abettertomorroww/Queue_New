@model GasColumn

<div class="modal fade" id="addEntry">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header-1">
                <h4 id="addEntryLabel" style="margin-top: -0.7rem; line-height: 1.5;">Ваш код записи</h4>
                <button type="button" class="close" data-dismiss="modal" style="padding: 1rem 1rem; margin:-1.3rem -1rem -1rem auto">
                    <span>x</span>
                </button>
            </div>

            <div class="modal-body-1">
                <form action="SingUpCodeGenerate">
                    <div class="form-group">
                        <label asp-for="Code">@Model.Code</label>
                    </div>
                </form>
            </div>

            <div class="modal-footer-1">
                <button class="modalBtn" type="button" data-dismiss="modal"> <span>Закрыть</span></button>
            </div>

        </div>
    </div>
</div>